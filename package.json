{
  "name": "reds-tone",
  "version": "0.1.0",
  "description": "Music in Minecraft 1.14+ datapacks.",
  "keywords": [
    "minecraft",
    "resourcepack",
    "datapack",
    "function",
    "redstone",
    "music"
  ],
  "homepage": "https://github.com/sjx233/reds-tone#readme",
  "bugs": "https://github.com/sjx233/reds-tone/issues",
  "license": "MIT",
  "author": "sjx233",
  "main": "lib/index.js",
  "typings": "lib/index",
  "bin": "./lib/cli.js",
  "directories": {
    "lib": "lib",
    "test": "lib/test"
  },
  "repository": "github:sjx233/reds-tone",
  "scripts": {
    "lint": "tslint --project .",
    "lint-fix": "tslint --project . --fix",
    "build": "rm -rf lib && tsc",
    "test": "node lib/test/test.js && node lib/cli.js -o test/cli -d \"CLI Test.\" -f \"clitest:play\" -g \"cli_test\" test/cli.mid",
    "preversion": "npm run build && npm test",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "commander": "^2.19.0",
    "midievents": "^2.0.0",
    "midifile": "^2.0.0",
    "minecraft-packs": "^1.0.1",
    "progress": "^2.0.3",
    "resource-location": "^2.2.1",
    "task-function": "^1.0.1",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/node": "^10.12.18",
    "@types/progress": "^2.0.3",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  },
  "engines": {
    "node": ">=7.6.0"
  }
}
